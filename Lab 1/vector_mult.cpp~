#include "stdio.h"
#include "omp.h"
#include <iostream>
#include <stdlib.h>
#include <time.h>

using namespace std;

int main () {
	
	int n = 10;
	
	int vec1[n];
	int vec2[n];

	srand(time(NULL));

	for(int i = 0;i < n;i++){
		for(int j = 0;j < n;j++){
			vec1[i] = rand() % 10 + 1;
			vec2[i]= rand() % 10 + 1;
		}
	}

	double start = omp_get_wtime();

	int total = 0;

	omp_set_dynamic(0); 
	omp_set_num_threads(4);
	#pragma omp parallel for
		for(int	i=0;i<n;i++){
			total += vec1[i] * vec2[i];
	}

	double end = omp_get_wtime();

	printf("Time: (%f)\n", end - start);
	
}
