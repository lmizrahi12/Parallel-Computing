#include "stdio.h"
#include "omp.h"
#include <iostream>
#include <stdlib.h>
#include <time.h>

using namespace std;

int main () {
	int n = 1000;
	
	int mat[n][n];

	srand(time(NULL));
	
	double start = omp_get_wtime();	

	for(int i = 0;i < n;i++){
		for(int j = 0;j < n;j++){
			mat[j][i] = rand() % 100 + 1;
		}
	}

	double end = omp_get_wtime();
	printf("Time: (%f)\n",end - start);
}
